{
  "name": "fe-scaffold-ssr",
  "version": "0.0.2",
  "description": "前端服务端渲染脚手架（egg 2.x + umi 3.x + antd 4.x）",
  "private": true,
  "scripts": {
    "dev": "egg-bin dev --port=7001 --sticky",
    "build": "cross-env APP_ROOT=app/web umi build",
    "start": "egg-scripts start --port=7001 --daemon --title=fe-scaffold-ssr --env=prod",
    "stop": "egg-scripts stop --title=fe-scaffold-ssr",
    "debug": "cross-env RM_TMPDIR=none COMPRESS=none egg-bin debug",
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && npm run cov",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./app/web && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./app/web",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"app/web/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "autod": "autod"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.5.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.0",
    "egg": "^2.12.0",
    "egg-scripts": "^2.10.0",
    "egg-view-assets": "^1.3.0",
    "egg-view-nunjucks": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "^15.6.2",
    "qs": "^6.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.4",
    "umi": "^3.2.15",
    "umi-request": "^1.3.5",
    "url": "^0.11.0",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "^2.2.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.5.17",
    "@umijs/preset-ui": "^2.0.9",
    "@umijs/yorkie": "^2.0.3",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-plugin-dva-hmr": "^0.4.0",
    "babel-plugin-import": "^1.12.0",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "egg-bin": "^4.9.0",
    "egg-http-proxy": "^1.0.1",
    "egg-mock": "^3.20.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.7.0",
    "eslint-config-egg": "^8.0.1",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.2.11",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.5",
    "pro-download": "1.0.1",
    "puppeteer-core": "^5.0.0",
    "redbox-react": "^1.5.0",
    "stylelint": "^13.6.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "feehc",
  "license": "MIT"
}
